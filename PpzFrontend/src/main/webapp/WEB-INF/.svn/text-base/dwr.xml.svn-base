<!DOCTYPE dwr PUBLIC
    "-//GetAhead Limited//DTD Direct Web Remoting 2.0//EN"
    "http://directwebremoting.org/schema/dwr20.dtd">
<dwr>
	<allow>
		<create creator="spring" javascript="PpzAjaxComponentJS">
			<param name="beanName" value="ppzAjaxComponent" />
			<include method="getJson" />
			<include method="getAvatarDetailJson" />
			<include method="getFamilyDetailJson" />
		</create>
		<create creator="spring" javascript="FamilySetupControllerJS">
			<param name="beanName" value="familySetupController" />
			<include method="edit" />
		</create>		
		<create creator="spring" javascript="BankControllerJS">
			<param name="beanName" value="bankController" />
			<include method="edit" />
		</create>			
		
	</allow>
</dwr>